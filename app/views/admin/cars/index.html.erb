<div class="table-responsive table-striped caption-top">
  <caption>List of cars</caption>
  <table class="table align-middle">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Car</th>
        <th scope="col">Price</th>
        <th scope="col">Rented</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @cars.each do |car| %>
      <tr class="align-bottom">
        <td><%= car.id %></td>
        <td><%= car.name %></td>
        <td><%= car.price %></td>
        <td><%= car.rented %></td>
        <td><%= link_to 'Show', admin_car_path(car) %></td>
        <td><%= link_to 'Edit', edit_admin_car_path(car) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>